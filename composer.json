{
  "name": "tendopay/laravel-saltedge-api",
  "description": "SaltEdge API Integration",
  "type": "library",
  "authors": [
    {
      "name": "Robert Kłodziński",
      "email": "robert.klodzinski@gmail.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "laravel/lumen-framework": "^6.0",
    "guzzlehttp/guzzle": "~6.0",
    "ext-json": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "orchestra/testbench": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "TendoPay\\Integration\\SaltEdge\\": "src/TendoPay/Integration/SaltEdge"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TendoPay\\Integration\\SaltEdge\\": "test/TendoPay/Integration/SaltEdge"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TendoPay\\Integration\\SaltEdge\\SaltEdgeServiceProvider"
      ]
    }
  }
}
